{
  "name": "shallow-differ",
  "version": "0.1.0",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/chodorowicz/shallow-differs"
  },
  "description": "compare two objects in a shallow way to check if they differ with support white list of properties",
  "main": "dist/index.js",
  "scripts": {
    "build": "microbundle",
    "dev": "microbundle watch",
    "test": "jest"
  },
  "source": "src/index.ts",
  "author": "",
  "dependencies": {},
  "devDependencies": {
    "@types/jest": "^23.3.2",
    "jest": "^23.6.0",
    "microbundle": "^0.6.0",
    "ts-jest": "^23.1.4",
    "typescript": "^3.0.3"
  },
  "typings": "dist/index.d.ts",
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "(src/__tests__/.*|(\\.|/)(test|spec))\\.ts$",
    "moduleFileExtensions": [
      "js",
      "ts"
    ]
  }
}
